/* pages/map-control/map-control.wxss */
.map-control-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  background: #F8F8F7;
  width: 100%;
  height: 100%;
}


.map {
  position: relative;
  width: 100%;
  height: 100%;
}

.cover-view {
  position: absolute;
  bottom: 80rpx;
  left: 50rpx;
}


.btn {
  background: #fff;
  margin-top: 10rpx;
  padding: 10rpx 20rpx;
  display: flex;
  align-items: center;
}

.icon {
  margin-right: 10rpx;
  display: inline-flex;
  width: 1em;
  height: 1em;
  overflow: visible;
  filter: contrast(0);
}